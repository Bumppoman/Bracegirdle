<div 
  class="container bracegirdle-pdf-viewer-wrapper"
  data-controller="pdfviewer"
>
  <div class="container bracegirdle-pdf-viewer bracegirdle-pdf-loading" data-target="pdfviewer.viewer">
    <div class="bracegirdle-pdf-viewer-menu row">
      <div class="col-md-3">
        <button class="centered-item">
          <i class="material-icons">cloud_download</i>
        </button>
      </div>
      <div class="col-md-6 text-center">
        <div class="group page">
          <button
            class="centered-item me-3" 
            data-action="pdfviewer#scrollBack"
            data-target="pdfviewer.previousPageButton"
          >
            <i class="material-icons">chevron_left</i>
          </button>
          <div class="group page">
            <span class="centered-item">Page</span>
            <input type="text" class="me-2" size="2" data-target="pdfviewer.currentPage" />
            <span class="group page ms-1">
              <span class="centered-item me-2">/</span>
              <span class="centered-item" data-target="pdfviewer.totalPages"></span>
            </span>
          </div>
          <button
            class="centered-item ms-3" 
            data-action="pdfviewer#scrollAhead"
            data-target="pdfviewer.nextPageButton"
          >
            <i class="material-icons">chevron_right</i>
          </button>
        </div>
      </div>
      <div class="col-md-6 col-lg-3 text-right">
        <div class="group zoom">
          <button 
            class="centered-item me-2" 
            data-action="pdfviewer#zoomOut"
            data-target="pdfviewer.zoomOutButton"
          >
            <i class="material-icons">zoom_out</i>
          </button>
          <button 
            class="centered-item me-2" 
            data-action="pdfviewer#zoomIn"
            data-target="pdfviewer.zoomInButton"
          >
            <i class="material-icons">zoom_in</i>
          </button>
          <input type="text" class="me-0" size="5" data-target="pdfviewer.zoomPercentage" />
        </div>
      </div>
    </div>
    <div 
      class="container bracegirdle-pdf-document"
      data-document-url="<%= defined?(url) ? url : pdf.url %>"
